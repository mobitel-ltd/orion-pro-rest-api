{
    "recurseDepth": 10,
    "source": {
        "include": ["./src"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/docs)"
    },
    "plugins": [
        "plugins/markdown",
        "node_modules/better-docs/category"
    ],
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "encoding": "utf8",
        "destination": "docs/",
        "readme": "README.md",
        "recurse": true,
        "verbose": true,
        "template": "node_modules/better-docs"
    }
}